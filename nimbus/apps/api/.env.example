

# Nimbus API example environment
NIMBUS_DATABASE_URL=postgresql+asyncpg://postgres:yourpassword@db:5432/nimbus

# For backend tests, use:
# NIMBUS_DATABASE_URL=postgresql+asyncpg://postgres:baha123@localhost:5432/nimbus_test
NIMBUS_REDIS_URL=redis://nimbus-redis:6379/0
NIMBUS_JWT_SECRET=your-super-secret-jwt-key-minimum-32-characters-long-for-security
NIMBUS_RATE_LIMIT_PER_MINUTE=120
NIMBUS_INGEST_API_KEY_ID=your-key-id
